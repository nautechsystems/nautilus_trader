# Environment Variables for NautilusTrader
# Copy this file to .env and update with your actual values

# Build Configuration
ANNOTATION_MODE=false
BUILD_MODE=release
CARGO_BUILD_TARGET=
CARGO_INCREMENTAL=0
CARGO_TARGET_DIR=./target
CC=clang
CFLAGS=
COPY_TO_SOURCE=true
CXX=clang++
DRY_RUN=false
DYLD_LIBRARY_PATH=
HIGH_PRECISION=true
LD_LIBRARY_PATH=
LDFLAGS=
LDSHARED=clang -shared
LIBRARY_PATH=
PARALLEL_BUILD=true
PROFILE_MODE=false
PYO3_ONLY=false
PYTHON_LIB_DIR=
RUSTFLAGS=
RUSTUP_TOOLCHAIN=stable

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Betfair API
BETFAIR_APP_KEY=your_betfair_app_key
BETFAIR_CERTS_DIR=/path/to/betfair/certs
BETFAIR_PASSWORD=your_betfair_password
BETFAIR_USERNAME=your_betfair_username

# Binance API
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret
BINANCE_FUTURES_API_KEY=your_binance_futures_api_key
BINANCE_FUTURES_API_SECRET=your_binance_futures_api_secret
BINANCE_FUTURES_TESTNET_API_KEY=your_binance_futures_testnet_api_key
BINANCE_FUTURES_TESTNET_API_SECRET=your_binance_futures_testnet_api_secret

# Bybit API
BYBIT_API_KEY=your_bybit_api_key
BYBIT_API_SECRET=your_bybit_api_secret
BYBIT_TESTNET_API_KEY=your_bybit_testnet_api_key
BYBIT_TESTNET_API_SECRET=your_bybit_testnet_api_secret

# CI/CD Configuration
CMAKE_C_COMPILER_LAUNCHER=sccache
CMAKE_CXX_COMPILER_LAUNCHER=sccache
CLOUDFLARE_R2_URL=your_cloudflare_r2_url

# Databento API
DATABENTO_API_KEY=your_databento_api_key

# GitHub Configuration
GITHUB_ENV=/path/to/github/env

# Interactive Brokers Configuration
IB_MAX_CONNECTION_ATTEMPTS=0
TWS_ACCOUNT=your_tws_account_id
TWS_PASSWORD=your_tws_password
TWS_USERNAME=your_tws_username
TWS_USERID=your_tws_user_id

# Memory & Performance
MEMORY_LEAK_THRESHOLD_BYTES_PY=10485760

# Data Paths
NAUTILUS_PATH=/path/to/nautilus/data

# OKX API
OKX_API_KEY=your_okx_api_key
OKX_API_SECRET=your_okx_api_secret

# Polygon/Polymarket Configuration
POLYGON_PRIVATE_KEY=your_polygon_private_key
POLYGON_PUBLIC_KEY=your_polygon_public_key
POLYMARKET_FUNDER=your_polymarket_funder
POLYMARKET_PK=your_polymarket_private_key

# PostgreSQL Database
POSTGRES_DATABASE=nautilus
POSTGRES_HOST=localhost
POSTGRES_PASSWORD=pass
POSTGRES_PORT=5432
POSTGRES_USERNAME=nautilus

# PyPI Publishing
PYPI_TOKEN=your_pypi_token
PYPI_USERNAME=__token__

# RPC Configuration
RPC_HTTP_URL=https://arb1.arbitrum.io/rpc
RPC_WSS_URL=wss://arb1.arbitrum.io/ws

# Rust/Cargo Build Cache
RUSTC_WRAPPER=sccache

# SCC Cache Configuration
SCCACHE_CACHE_MULTIARCH=1
SCCACHE_CACHE_SIZE=4G
SCCACHE_DIR=/path/to/sccache/cache
SCCACHE_DIRECT=true
SCCACHE_IDLE_TIMEOUT=0

# Tardis Machine/API
TARDIS_API_KEY=your_tardis_api_key
TARDIS_MACHINE_WS_URL=ws://localhost:8001
TM_API_KEY=your_tm_api_key

# UV Package Manager
UV_CACHE_DIR=/path/to/uv/cache
UV_PUBLISH_PASSWORD=your_pypi_token
UV_PUBLISH_USERNAME=__token__
UV_VERSION=0.4.18

# Virtual Environment
VIRTUAL_ENV=/path/to/virtual/env
